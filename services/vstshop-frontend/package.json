{
  "name": "vstshop-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
"scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy-site": "aws s3 sync dist/ s3://vstshop-frontend-dev123 --delete",
    "deploy-assets": "aws s3 sync src/images/products s3://vstshop-frontend-dev123/product-media --content-type image/png",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E228GMNSTNHTAD --paths '/*'",
    "deploy": "npm run build && npm run deploy-site && npm run deploy-assets && npm run invalidate"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^6.15.0",
    "aws-amplify": "^6.16.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "vite": "^7.3.1"
  }
}
